{
  "name": "ultramarine-vscodeextension",
  "displayName": "ultramarine.vscodeextension",
  "description": "code analyzers and generator platform",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ultramarine.showGeneratorEditor",
    "onCommand:ultramarine.runProjectGenerator"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ultramarine.showGeneratorEditor",
        "title": "Show Generator Editor"
      },
      {
        "command": "ultramarine.runProjectGenerator",
        "title": "Run Generator"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename == Project.gen.json",
          "command": "ultramarine.showGeneratorEditor",
          "group": "7_modification"
        },
        {
          "when": "resourceFilename == Project.gen.json",
          "command": "ultramarine.runProjectGenerator",
          "group": "7_modification"
        }
      ]
    }
  },
  "dependencies": {
    "materialize-css": "^1.0.0-rc.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-hot-loader": "^4.8.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "concurrently": "^4.1.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16.3": "^1.7.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.12.4",
    "express": "^4.17.3",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.16.4",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "resolve": "1.10.0",
    "rimraf": "^2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "vscode": "^1.1.28",
    "webpack": "4.28.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3",
    "cross-env": "^5.2.0"
  },
  "scripts": {
    "build": "npm run clean && npm run release",
    "start": "webpack-dev-server --config webpack.develop.js",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "clean": "rimraf ./build",
    "release": "cross-env NODE_ENV=production webpack --config ./webpack.production.js -p --progress --color",
    "web": "concurrently \"node server.js\" \"npm run web:start\"",
    "web:start": "webpack-dev-server --config webpack.web.js",
    "test": "node ./node_modules/vscode/bin/test",
    "test:components": "jest src --config jest.config.json",
    "pretty": "prettier --check src/**/*.js",
    "coverage:components": "jest --coverage src --config jest.config.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
